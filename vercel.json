{
  "version": 2,
  "builds": [
    {
      "src": "dunamis/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dunamis/wsgi.py"
    }
  ],
  "redirects": [
    {
      "source": "http://dunamislawfirm.com/:path*",
      "destination": "https://www.dunamislawfirm.com/:path*",
      "permanent": true
    },
    {
      "source": "https://dunamislawfirm.com/:path*",
      "destination": "https://www.dunamislawfirm.com/:path*",
      "permanent": true
    }
  ]
}
